{
  "name": "ilatex",
  "version": "1.0.0",
  "main": "./release/dist/main/index.js",
  "scripts": {
    "dev": "npm run dev:render",
    "dev:render": "rsbuild dev --config ./builder/rsbuild.render.ts",
    "dev:main": "nodemon",
    "build:main": "rsbuild build --config ./builder/rsbuild.main.ts",
    "prepare": "npx simple-git-hooks",
    "lint": "pnpm biome check --write ./src",
    "cz": "git add . && git-cz"
  },
  "author": "moki 2893096286@qq.com",
  "bugs": {
    "url": "https://github.com/ClearLuvMoki/ILatex/issues"
  },
  "homepage": "https://github.com/ClearLuvMoki/ILatex#readme",
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "pnpm run lint"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint --edit ${1}",
    "preserveUnused": [
      "commit-msg"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@rsbuild/core": "^1.6.6",
    "@rsbuild/plugin-babel": "^1.0.6",
    "@rsbuild/plugin-react": "^1.4.2",
    "@sentry/electron": "^7.2.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "electron": "^39.1.2",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "fs-extra": "^11.3.2",
    "lucide-react": "^0.553.0",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "posthog-node": "^5.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "simple-git-hooks": "^2.13.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "next-themes": "^0.4.6",
    "react-hook-form": "^7.66.0"
  }
}
